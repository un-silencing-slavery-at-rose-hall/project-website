<g class="frond" {{build-frond this.stem this.leaves}}>
  <path class="frond-stem" d={{this.path}} stroke="black"
    {{on-resize this.pathResize}}
    fill="none"
    />
  <g id="frondpoints"/>
  <g class="dots">
    {{#each this.leaves as |dot|}}
      <circle r="5" cx={{dot.x}} cy={{dot.y}} />
    {{/each}}
  </g>
  <text transform="translate(150, 150)">{{this.svg.width}} || {{this.svg.frondLength}}</text>
</g>

